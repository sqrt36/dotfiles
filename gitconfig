    name = Vance Willbanks

[color]
    branch = auto
    diff = auto
    status = auto

[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green

[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold

[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[alias]
    l = log --graph --pretty=format:'%C(yellow)%H %Creset%C(cyan)%aN%Creset %C(green)%ar %n %b %w(70) %s%n' --color=auto --stat
